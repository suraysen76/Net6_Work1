@model IEnumerable<Category>
<div class="text-center">
    <h1 class="display-4">Category Page</h1>
    <table class="table-bordered table-striped" style="width:100%">
        <tr>
            <th>Name</th>
            <th>Display Order</th>
        </tr>
        @foreach(var obj in Model)
        {
            <tr>
                <td>@obj.Name</td>
                <td>@obj.DisplayOrder</td>
            </tr>
        }
    </table>
</div>